    "@admin/module": "dist/src/modules/admin/admin.module.js",
    "@admin/controller": "dist/src/modules/admin/controller/admin.controller",
    "@borrowbooks/module": "dist/src/modules/borrowbook/borrowbook.module.js",
    "@member/module": "dist/src/modules/members/member.module.js",
    "@member/controller": "dist/src/modules/members/controller/member.controller.js",
    "@books/module": "dist/src/modules/books/book.module.js",
