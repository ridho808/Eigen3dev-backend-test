    "@types/swagger-ui-express": "^4.1.6",
    "jest": "^29.7.0",
    "moment": "^2.30.1",
    "prisma": "^5.19.1",
    "supertest": "^7.0.0",
    "swagger-jsdoc": "^6.2.8",
